{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "types": ["node"]
  },
  "include": ["scripts/**/*.ts", "ingest/**/*.ts", "ingest/**/*.d.ts"],
  "exclude": ["node_modules"]
}

